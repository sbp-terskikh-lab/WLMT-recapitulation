       && test -n "$RTD" \
       && (test "$enable_man" = yes || test "$enable_html" = yes); then :
  have_docs=yes
else
  have_docs=no
fi
if    test $enable_html = yes && test $have_docs = no; then :
  { $as_echo "$as_me:${as_lineno-$LINENO}: WARNING: forcing --disable-html: no suitable sphinx-build" >&5
$as_echo "$as_me: WARNING: forcing --disable-html: no suitable sphinx-build" >&2;}
       enable_html=no
fi
if    test $enable_man = yes && test $have_docs = no; then :
  { $as_echo "$as_me:${as_lineno-$LINENO}: WARNING: forcing --disable-man: no suitable sphinx-build" >&5
$as_echo "$as_me: WARNING: forcing --disable-man: no suitable sphinx-build" >&2;}
       enable_man=no
fi


### Glue scripts #############################################################

# Buildah
vmin_buildah=1.11.2
# Extract the first word of "buildah", so it can be a program name with args.
set dummy buildah; ac_word=$2
{ $as_echo "$as_me:${as_lineno-$LINENO}: checking for $ac_word" >&5
$as_echo_n "checking for $ac_word... " >&6; }
if ${ac_cv_prog_BUILDAH+:} false; then :
  $as_echo_n "(cached) " >&6
else
  if test -n "$BUILDAH"; then
  ac_cv_prog_BUILDAH="$BUILDAH" # Let the user override the test.
else
as_save_IFS=$IFS; IFS=$PATH_SEPARATOR
for as_dir in $PATH
do
  IFS=$as_save_IFS
  test -z "$as_dir" && as_dir=.
    for ac_exec_ext in '' $ac_executable_extensions; do
  if as_fn_executable_p "$as_dir/$ac_word$ac_exec_ext"; then
    ac_cv_prog_BUILDAH="buildah"
    $as_echo "$as_me:${as_lineno-$LINENO}: found $as_dir/$ac_word$ac_exec_ext" >&5
    break 2
  fi
done
  done
IFS=$as_save_IFS

fi
fi
BUILDAH=$ac_cv_prog_BUILDAH
if test -n "$BUILDAH"; then
  { $as_echo "$as_me:${as_lineno-$LINENO}: result: $BUILDAH" >&5
$as_echo "$BUILDAH" >&6; }
else
  { $as_echo "$as_me:${as_lineno-$LINENO}: result: no" >&5
$as_echo "no" >&6; }
fi




  if test -n "$BUILDAH"; then :

    # ${!1} is non-empty
    case $BUILDAH in #(
  # absolute path; check if executable
      /*) :
    { $as_echo "$as_me:${as_lineno-$LINENO}: checking if $BUILDAH is executable" >&5
$as_echo_n "checking if $BUILDAH is executable... " >&6; }
             if test -e "$BUILDAH"; then :
  { $as_echo "$as_me:${as_lineno-$LINENO}: result: ok" >&5
$as_echo "ok" >&6; }
else
  { $as_echo "$as_me:${as_lineno-$LINENO}: result: no" >&5
$as_echo "no" >&6; }
               as_fn_error $? "must be executable" "$LINENO" 5
fi ;; #(
  *) :
     ;;
esac
    { $as_echo "$as_me:${as_lineno-$LINENO}: checking if $BUILDAH version >= $vmin_buildah" >&5
$as_echo_n "checking if $BUILDAH version >= $vmin_buildah... " >&6; }
    vact=$($BUILDAH --version | cut -d' ' -f3)



  # Used to indicate true or false condition
  ax_compare_version=false

  # Convert the two version strings to be compared into a format that
  # allows a simple string comparison.  The end result is that a version
  # string of the form 1.12.5-r617 will be converted to the form
  # 0001001200050617.  In other words, each number is zero padded to four
  # digits, and non digits are removed.

  ax_compare_version_A=`echo "$vmin_buildah" | sed -e 's/\([0-9]*\)/Z\1Z/g' \
                     -e 's/Z\([0-9]\)Z/Z0\1Z/g' \
                     -e 's/Z\([0-9][0-9]\)Z/Z0\1Z/g' \
                     -e 's/Z\([0-9][0-9][0-9]\)Z/Z0\1Z/g' \
                     -e 's/[^0-9]//g'`


  ax_compare_version_B=`echo "$vact" | sed -e 's/\([0-9]*\)/Z\1Z/g' \
                     -e 's/Z\([0-9]\)Z/Z0\1Z/g' \
                     -e 's/Z\([0-9][0-9]\)Z/Z0\1Z/g' \
                     -e 's/Z\([0-9][0-9][0-9]\)Z/Z0\1Z/g' \
                     -e 's/[^0-9]//g'`


    ax_compare_version=`echo "x$ax_compare_version_A
x$ax_compare_version_B" | sed 's/^ *//' | sort | sed "s/x${ax_compare_version_A}/true/;s/x${ax_compare_version_B}/false/;1q"`



    if test "$ax_compare_version" = "true" ; then

      BUILDAH_VERSION_NOTE="ok ($vact)"

      { $as_echo "$as_me:${as_lineno-$LINENO}: result: ok ($vact)" >&5
$as_echo "ok ($vact)" >&6; }

    else
      BUILDAH_VERSION_NOTE="too old ($vact)"

      { $as_echo "$as_me:${as_lineno-$LINENO}: result: too old ($vact)" >&5
$as_echo "too old ($vact)" >&6; }
      { BUILDAH=; unset BUILDAH;}

  fi


else

    # ${!} is empty
    BUILDAH_VERSION_NOTE="not found"

    { BUILDAH=; unset BUILDAH;}

fi



