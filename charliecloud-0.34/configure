# SquashFS Tools
vmin_mksquashfs=4.2  # CentOS 7
# Extract the first word of "mksquashfs", so it can be a program name with args.
set dummy mksquashfs; ac_word=$2
{ $as_echo "$as_me:${as_lineno-$LINENO}: checking for $ac_word" >&5
$as_echo_n "checking for $ac_word... " >&6; }
if ${ac_cv_prog_MKSQUASHFS+:} false; then :
  $as_echo_n "(cached) " >&6
else
  if test -n "$MKSQUASHFS"; then
  ac_cv_prog_MKSQUASHFS="$MKSQUASHFS" # Let the user override the test.
else
as_save_IFS=$IFS; IFS=$PATH_SEPARATOR
for as_dir in $PATH
do
  IFS=$as_save_IFS
  test -z "$as_dir" && as_dir=.
    for ac_exec_ext in '' $ac_executable_extensions; do
  if as_fn_executable_p "$as_dir/$ac_word$ac_exec_ext"; then
    ac_cv_prog_MKSQUASHFS="mksquashfs"
    $as_echo "$as_me:${as_lineno-$LINENO}: found $as_dir/$ac_word$ac_exec_ext" >&5
    break 2
  fi
done
  done
IFS=$as_save_IFS

fi
fi
MKSQUASHFS=$ac_cv_prog_MKSQUASHFS
if test -n "$MKSQUASHFS"; then
  { $as_echo "$as_me:${as_lineno-$LINENO}: result: $MKSQUASHFS" >&5
$as_echo "$MKSQUASHFS" >&6; }
else
  { $as_echo "$as_me:${as_lineno-$LINENO}: result: no" >&5
$as_echo "no" >&6; }
fi




  if test -n "$MKSQUASHFS"; then :

    # ${!1} is non-empty
    case $MKSQUASHFS in #(
  # absolute path; check if executable
      /*) :
    { $as_echo "$as_me:${as_lineno-$LINENO}: checking if $MKSQUASHFS is executable" >&5
$as_echo_n "checking if $MKSQUASHFS is executable... " >&6; }
             if test -e "$MKSQUASHFS"; then :
  { $as_echo "$as_me:${as_lineno-$LINENO}: result: ok" >&5
$as_echo "ok" >&6; }
else
  { $as_echo "$as_me:${as_lineno-$LINENO}: result: no" >&5
$as_echo "no" >&6; }
               as_fn_error $? "must be executable" "$LINENO" 5
fi ;; #(
  *) :
     ;;
esac
    { $as_echo "$as_me:${as_lineno-$LINENO}: checking if $MKSQUASHFS version >= $vmin_mksquashfs" >&5
$as_echo_n "checking if $MKSQUASHFS version >= $vmin_mksquashfs... " >&6; }
    vact=$($MKSQUASHFS -version | head -1 | cut -d' ' -f3)



  # Used to indicate true or false condition
  ax_compare_version=false

  # Convert the two version strings to be compared into a format that
  # allows a simple string comparison.  The end result is that a version
  # string of the form 1.12.5-r617 will be converted to the form
  # 0001001200050617.  In other words, each number is zero padded to four
  # digits, and non digits are removed.

  ax_compare_version_A=`echo "$vmin_mksquashfs" | sed -e 's/\([0-9]*\)/Z\1Z/g' \
                     -e 's/Z\([0-9]\)Z/Z0\1Z/g' \
                     -e 's/Z\([0-9][0-9]\)Z/Z0\1Z/g' \
                     -e 's/Z\([0-9][0-9][0-9]\)Z/Z0\1Z/g' \
                     -e 's/[^0-9]//g'`


  ax_compare_version_B=`echo "$vact" | sed -e 's/\([0-9]*\)/Z\1Z/g' \
                     -e 's/Z\([0-9]\)Z/Z0\1Z/g' \
                     -e 's/Z\([0-9][0-9]\)Z/Z0\1Z/g' \
                     -e 's/Z\([0-9][0-9][0-9]\)Z/Z0\1Z/g' \
                     -e 's/[^0-9]//g'`


    ax_compare_version=`echo "x$ax_compare_version_A
x$ax_compare_version_B" | sed 's/^ *//' | sort | sed "s/x${ax_compare_version_A}/true/;s/x${ax_compare_version_B}/false/;1q"`



    if test "$ax_compare_version" = "true" ; then

      MKSQUASHFS_VERSION_NOTE="ok ($vact)"

      { $as_echo "$as_me:${as_lineno-$LINENO}: result: ok ($vact)" >&5
$as_echo "ok ($vact)" >&6; }

    else
      MKSQUASHFS_VERSION_NOTE="too old ($vact)"

      { $as_echo "$as_me:${as_lineno-$LINENO}: result: too old ($vact)" >&5
$as_echo "too old ($vact)" >&6; }
      { MKSQUASHFS=; unset MKSQUASHFS;}

  fi


else

    # ${!} is empty
    MKSQUASHFS_VERSION_NOTE="not found"

    { MKSQUASHFS=; unset MKSQUASHFS;}

fi



