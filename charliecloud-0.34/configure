    { $as_echo "$as_me:${as_lineno-$LINENO}: checking if $SQUASHFUSE is executable" >&5
$as_echo_n "checking if $SQUASHFUSE is executable... " >&6; }
             if test -e "$SQUASHFUSE"; then :
  { $as_echo "$as_me:${as_lineno-$LINENO}: result: ok" >&5
$as_echo "ok" >&6; }
else
  { $as_echo "$as_me:${as_lineno-$LINENO}: result: no" >&5
$as_echo "no" >&6; }
               as_fn_error $? "must be executable" "$LINENO" 5
fi ;; #(
  *) :
     ;;
esac
    { $as_echo "$as_me:${as_lineno-$LINENO}: checking if $SQUASHFUSE version >= $vmin_squashfuse" >&5
$as_echo_n "checking if $SQUASHFUSE version >= $vmin_squashfuse... " >&6; }
    vact=$($SQUASHFUSE --help 2>&1 | head -1 | cut -d' ' -f2)



  # Used to indicate true or false condition
  ax_compare_version=false

  # Convert the two version strings to be compared into a format that
  # allows a simple string comparison.  The end result is that a version
  # string of the form 1.12.5-r617 will be converted to the form
  # 0001001200050617.  In other words, each number is zero padded to four
  # digits, and non digits are removed.

  ax_compare_version_A=`echo "$vmin_squashfuse" | sed -e 's/\([0-9]*\)/Z\1Z/g' \
                     -e 's/Z\([0-9]\)Z/Z0\1Z/g' \
                     -e 's/Z\([0-9][0-9]\)Z/Z0\1Z/g' \
                     -e 's/Z\([0-9][0-9][0-9]\)Z/Z0\1Z/g' \
                     -e 's/[^0-9]//g'`


  ax_compare_version_B=`echo "$vact" | sed -e 's/\([0-9]*\)/Z\1Z/g' \
                     -e 's/Z\([0-9]\)Z/Z0\1Z/g' \
                     -e 's/Z\([0-9][0-9]\)Z/Z0\1Z/g' \
                     -e 's/Z\([0-9][0-9][0-9]\)Z/Z0\1Z/g' \
                     -e 's/[^0-9]//g'`


    ax_compare_version=`echo "x$ax_compare_version_A
x$ax_compare_version_B" | sed 's/^ *//' | sort | sed "s/x${ax_compare_version_A}/true/;s/x${ax_compare_version_B}/false/;1q"`



    if test "$ax_compare_version" = "true" ; then

      SQUASHFUSE_VERSION_NOTE="ok ($vact)"

      { $as_echo "$as_me:${as_lineno-$LINENO}: result: ok ($vact)" >&5
$as_echo "ok ($vact)" >&6; }

    else
      SQUASHFUSE_VERSION_NOTE="too old ($vact)"

      { $as_echo "$as_me:${as_lineno-$LINENO}: result: too old ($vact)" >&5
$as_echo "too old ($vact)" >&6; }
      { SQUASHFUSE=; unset SQUASHFUSE;}

  fi


else

    # ${!} is empty
    SQUASHFUSE_VERSION_NOTE="not found"

    { SQUASHFUSE=; unset SQUASHFUSE;}

fi



